/* CREATE DATABASE */

DROP DATABASE IF EXISTS `FIGHT IT PETS CONTROL`;
CREATE DATABASE `FIGHT IT PETS CONTROL`;
USE `FIGHT IT PETS CONTROL`;




/* CREATE TABLE FOR ALL TABLE */


CREATE TABLE office (
 
OFFICEID varchar(12) NOT NULL,
OFFICENAME varchar (50) NOT NULL,
OFFICENOTELL varchar(100) NOT NULL,
OFFICEADDRESS varchar(100) NOT NULL,
PRIMARY KEY (OFFICEID));

