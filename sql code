CREATE TABLE staff (


FOREIGN KEY (OFFICEID) REFERENCES office (OFFICEID),
STAFFNO varchar(10) NOT NULL,
STAFFNAME varchar(50) NOT NULL,
STAFFJOBTITLE varchar (12) NOT NULL,
STAFFDATEBIRTH date,
STAFFTYPE varchar (10),
STAFFMARRIEDDATE date,
OFFICEID varchar (12) NOT NULL,
SPOUSEID INT(10) NOT NULL,
PRIMARY KEY(STAFFNO)
);


