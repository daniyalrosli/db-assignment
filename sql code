/* ALTER TABLE (ADD & DROP COLUMN) */

ALTER TABLE staff
ADD COLUMN GENDER varchar(12) NOT NULL;

ALTER TABLE staff
DROP COLUMN GENDER;


select *from staff;



























/* RETRIEVING DATA FROM MULTIPLE TABLE */

SELECT  STAFFJOBTITLE, STAFFNAME, STAFFMARRIEDDATE
FROM staff
WHERE STAFFJOBTITLE = 'CLERK'
ORDER BY STAFFJOBTITLE, STAFFNAME, STAFFMARRIEDDATE;


SELECT STAFFNO,STAFFTYPE , STAFFNAME
FROM staff
WHERE STAFFTYPE = 'PART TIME'
ORDER BY STAFFNO, STAFFTYPE, STAFFNAME;



SELECT VENDORID, VENDORNAME, VENDORADDRESS
FROM vendor
WHERE VENDORNAME = 'Durham Shop' 
ORDER BY VENDORID, VENDORNAME, VENDORADDRESS;


SELECT VENDORTELNO
FROM vendor
WHERE VENDORADDRESS = '43 Amsterdam Holland'
ORDER BY VENDORTELNO;





SELECT OFFICENAME , OFFICENOTELL, OFFICEADDRESS
FROM office
WHERE OFFICEID = '5'
ORDER BY  OFFICENAME , OFFICENOTELL, OFFICEADDRESS DESC;










































/* COLUMN FUNCTION & GROUPING */


SELECT STAFFNO, STAFFNAME, STAFFDATEBIRTH
FROM staff
ORDER BY STAFFNO ASC;


SELECT max(CHEMICALPRICE) AS 'Highest Price', min(CHEMICALPRICE) AS'Lowest Price'
FROM chemical;

SELECT VENDORADDRESS AS 'VENDORS ADDRESS', COUNT(*) AS 'COUNT'
FROM vendor
GROUP BY VENDORADDRESS
ORDER BY VENDORADDRESS;



/* SCALAR FUNCTION */
SELECT STAFFNAME, concat(STAFFNO, "  ",STAFFTYPE) AS STAFFTYPE
FROM staff;


SELECT STAFFNAME, STAFFNO, STAFFDATEBIRTH
FROM staff
WHERE STAFFDATEBIRTH >= '1997-03-24'
ORDER BY STAFFDATEBIRTH;


SELECT CHEMICALID, CHEMICALNAME, CHEMICALPRICE, CHEMICALPRICE * 1.0375
FROM chemical
WHERE CHEMICALPRICE < '150'
ORDER BY CHEMICALID;



